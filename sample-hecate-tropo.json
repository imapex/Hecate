{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "DOCKERUSER/hecate-tropo:latest",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5001,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/DEPLOYMENTDIR/DEPLOYMENTNAME/DOCKERUSER/tropo",
    "instances": 1,
    "cpus": 0.1,
    "mem": 16,
    "env": {
      "MQTT_SERVER": "MQTTSERVER"
    }
}
